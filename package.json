    "eslint": "^8.49.0",
    "eslint-config-next": "^13.5.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6",
    "html-webpack-plugin": "5.5.0",
    "postcss": "^8.4.32",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "tailwindcss": "^3.4.0",
